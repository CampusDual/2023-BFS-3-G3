<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup
    xmlns="http://www.ontimize.com/schema/jdbc"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
    catalog="" schema="${mainschema}" table="gyms"
    datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>gymid</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>gymid</Column>
    </UpdateKeys>
    <Queries>
        <Query id="default">
            <ValidColumns>
                <Column>gymid</Column>
                <Column>name</Column>
                <Column>address</Column>
                <Column>city</Column>
                <Column>cif</Column>
                <Column>phone</Column>
                <Column>email</Column>
                <Column>schedule</Column>
                <Column>description</Column>
                <Column>state</Column>
                <Column>user_</Column>
                <Column>photo</Column>
            </ValidColumns>
            <Sentence>
                <![CDATA[
                    SELECT
                        #COLUMNS#
                    FROM
                        ${mainschema}.gyms
                    #WHERE#
                ]]>
            </Sentence>
        </Query>
    </Queries>
</JdbcEntitySetup>